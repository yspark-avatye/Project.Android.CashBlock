<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-2dp"
        android:bottom="-2dp" >
        <shape android:shape="rectangle" >
            <solid android:color="#F7F7F7" />

            <stroke
                android:width="1dp"
                android:color="#CED4DB" />
        </shape>
    </item>
    <item
        android:top="-2dp"
        android:right="-2dp"
        android:left="-2dp" >
        <shape android:shape="rectangle" >
            <stroke
                android:width="1dp"
                android:color="@color/acb_core_color_light_blue" />
        </shape>
    </item>
</layer-list>