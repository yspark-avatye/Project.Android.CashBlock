<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- region { global } -->
    <attr name="acbsr_widget_display_type" format="enum">
        <enum name="fill" value="0" />
        <enum name="stroke" value="1" />
    </attr>
    <!-- endregion -->


    <!-- region { WinnerDisplay } -->
    <declare-styleable name="WinnerDisplayView">
        <attr name="acbsr_widget_display_type" />
    </declare-styleable>
    <!-- endregion -->


    <!-- region # progress circle -->
    <declare-styleable name="CircleProgressView">
        <attr name="acbsr_widget_progress_border_width" format="dimension" />
        <attr name="acbsr_widget_progress_border_color" format="color" />
        <attr name="acbsr_widget_progress_thickness" format="dimension" />
        <attr name="acbsr_widget_progress_foreground_color" format="color" />
        <attr name="acbsr_widget_progress_background_color" format="color" />
        <attr name="acbsr_widget_progress_center_color" format="color" />
        <attr name="acbsr_widget_progress_value" format="integer" />
        <attr name="acbsr_widget_progress_max" format="integer" />
        <attr name="acbsr_widget_progress_min" format="integer" />
    </declare-styleable>
    <!-- endregion -->


</resources>